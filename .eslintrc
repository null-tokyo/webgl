{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-unused-vars": 1,
    "no-console": 1,
    "prettier/prettier": "warning"
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "globals": {
    "jQuery": true,
    "$": true,
    "window": true,
    "THREE": true,
    "GPUComputationRenderer": true
  }
}
